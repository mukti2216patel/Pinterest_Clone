<% include partials/header.ejs %>
        <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
            <div class="w-20vw flex-wrap flex gap-10 flex-row ">
                <%user.posts.forEach(function(elem){%>
                    <div class="cards flex flex-wrap gap-3">
                            <div class="card w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
                                    <img src="/images/uploads/<%= elem.postimage %>" class="w-full h-full object-cover" alt="">
                                </div>
                                <h5 class="mt-2 text-sm text-semibold "><%= elem.title %></h5>
    
                        </div>
                <%})%>
            </div>    
        </div>
<% include partials/footer.ejs %>